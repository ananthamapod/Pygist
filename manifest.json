{
  "name": "pygist",
  "version": "0.1",
  "description": "Tool for automatically running python in the browser console from a gist",
  "content_scripts": [
    {
    "matches": [
      "*://gist.githubusercontent.com/*.py"
    ],
    "js": [
      "jquery.min.js",
      "skulpt.min.js",
      "skulpt-stdlib.js",
      "run.js"
    ],
    "run_at": "document_start"
    }
  ],
  "icons": {
  "16": "logo-128.png",
  "48": "logo-128.png",
  "128": "logo-128.png"
  },
  "manifest_version": 2
}
